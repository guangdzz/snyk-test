{
  "name": "ant-design-pro",
  "group": "com.wacai.wodlee",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build:dll": "webpack --config webpack.dll.config.js",
    "build": "COMPRESS=none umi build",
    "scp": "sh script/scp.sh",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp && patch-package",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "cross-env APP_PORT=8000 MOCK=none umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env APP_PORT=8000 MOCK=none umi dev",
    "start:no-mock2": "cross-env APP_PORT=8001 MOCK=none umi dev --port=8001",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "patch-package": "patch-package",
    "webpack": "umi webpack",
    "start-mock": "cross-env APP_PORT=8000 umi dev"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "^1.0.2",
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^5.0.8",
    "@ant-design/pro-table": "2.2.7",
    "@antv/data-set": "^0.10.1",
    "@antv/g6": "^3.6.0",
    "@fud/gg-editor": "^3.1.5",
    "@types/papaparse": "^5.3.7",
    "@types/spark-md5": "^3.0.2",
    "antd": "^4.15.5",
    "babel": "^6.23.0",
    "bizcharts": "^3.4.3",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "brace": "^0.11.1",
    "child_process": "^1.0.2",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^1.0.1",
    "crypto-js": "^4.0.0",
    "eosjs-ecc": "^4.0.7",
    "file-saver": "^2.0.2",
    "form-render": "0.8.6",
    "gg-editor": "^3.1.1",
    "jsencrypt": "^3.0.0-rc.1",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "marked": "^0.6.3",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.2",
    "papaparse": "^5.2.0",
    "patch-package": "^6.2.2",
    "path-to-regexp": "2.4.0",
    "qiankun": "^2.4.9",
    "qs": "^6.9.0",
    "query-string": "^6.13.7",
    "react": "^17.0.2",
    "react-ace": "^9.5.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.2",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.4",
    "react-highlight-words": "^0.17.0",
    "react-syntax-highlighter": "^15.4.3",
    "readline": "^1.3.0",
    "umi": "3.1.2",
    "umi-request": "^1.0.8",
    "use-merge-value": "^1.0.1",
    "uuid": "^3.4.0",
    "webpack-cli": "^3.3.12",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.18",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/file-saver": "^2.0.1",
    "@types/history": "^4.7.2",
    "@types/jest": "^25.1.0",
    "@types/lodash": "^4.14.144",
    "@types/pako": "^1.0.1",
    "@types/qs": "^6.5.3",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/react-helmet": "^5.0.13",
    "@umijs/fabric": "^2.0.5",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/preset-ui": "^2.0.9",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "compression-webpack-plugin": "^6.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.0.7",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "node-loader": "^1.0.2",
    "prettier": "^2.0.1",
    "pro-download": "1.0.1",
    "puppeteer-core": "^3.0.0",
    "react-json-view": "^1.21.3",
    "spark-md5": "^3.0.1",
    "stylelint": "^13.0.0"
  },
  "engines": {
    "node": "10.14.1",
    "pm2": "3.2.2"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
